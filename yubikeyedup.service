[Unit]
Description=Yubikey Validation Server (python3 tidy version)
After=network.target
StartLimitIntervalSec=0

[Service]
Type=simple
Restart=on-failure
RestartSec=1
User=yubikeyval
ExecStart=/usr/bin/env python3 /home/yubikeyval/yubikeyedup-python3/src/yubiserve.py

[Install]
WantedBy=multi-user.target

